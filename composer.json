{
    "name": "jelix/adminui-module",
    "type": "jelix-module",
    "description": "module for a jelix application providing an admin interface made with the AdminLte theme. Other theme can be made",
    "keywords": ["admin", "module", "adminui", "adminlte"],
    "license": "MIT",
    "authors": [
        {
            "name": "Laurent Jouanneau",
            "email": "laurent@jelix.org"
        }
    ],
    "require": {
        "php": ">=5.6",
        "almasaeed2010/adminlte": "~3.2.0"
    },
    "archive": {
        "exclude": ["/test"]
    },
    "extra": {
        "jelix" : {
            "modules-dir": [
                "modules"
            ]
        },
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "minimum-stability": "stable"
}
